{% extends "layouts/base.njk" %} 
{% block content %}
  <article class="intro">
      <div>
          <h1 class="intro_header">
              {{ intro.eyebrow }}
              <em class="intro_main"> {{ intro.main }} </em>
          </h1>
      </div>
      <div class="intro_summary">
          {{ intro.summary}}
      </div>
      <img 
        class="intro_image"
        src="{{ intro.image }}"
        alt=" {{ intro.imageAlt }} "
    />
  </article>
{% endblock %}